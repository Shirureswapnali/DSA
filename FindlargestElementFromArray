import java.util.*;
class large
{
public static void main(String args[])
{
int[]a=new int[10];
int i;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the array element");
for(i=0;i<a.length;i++)
{
a[i]=sc.nextInt();
}
int k=a[0];
for(i=0;i<a.length;i++)
{
if(a[i]>k)
{
k=a[i];
}
}
System.out.println("largest element"+k);
}
}
