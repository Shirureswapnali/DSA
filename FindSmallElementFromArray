import java.util.*;
class small
{
public static void main(String args[])
{
 final int size=10;
int[]a=new int[size];
int i;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the array element");
for(i=0;i<size;i++)
{
a[i]=sc.nextInt();
}
int k=a[0];
for(i=0;i<size;i++)
{
if(a[i]<k)
{
k=a[i];
}
}
System.out.println("Smallest Element"+k);
}
}

